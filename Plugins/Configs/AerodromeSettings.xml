    <!-- MARK: Layout Definitions -->
    <!-- Select which bays should appear in which layouts, and which columns they should display in
    (requires that the bays are defined in BayTypes below -->
    <LayoutDefinitions>
        <!-- DEFAULT / RADAR TOWERS -->
        <LayoutDefinition Name="All">
            <Element Name="Preactive" Column="0" />
            <Element Name="Cleared" Column="0" />
            <Element Name="Monitor SMC" Column="0" />
            <Element Name="Pushback" Column="0" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="1" />
            <Element Name="Runway" Column="1" />
            <Element Name="Circuit" Column="2" />
            <Element Name="Departed" Column="2" />
            <Element Name="Arrivals" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="ACD">
            <Element Name="Preactive" Column="0" />
            <Element Name="Cleared" Column="1" />
            <Element Name="Monitor SMC" Column="2" />
            <Element Name="Pushback" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="ACD+SMC">
            <Element Name="Preactive" Column="0" />
            <Element Name="Cleared" Column="0" />
            <Element Name="Monitor SMC" Column="0" />
            <Element Name="Pushback" Column="1" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="2" />
            <Element Name="Runway" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="SMC">
            <Element Name="Cleared" Column="0" />
            <Element Name="Monitor SMC" Column="0" />
            <Element Name="Pushback" Column="1" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="2" />
            <Element Name="Runway" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="SMC+ADC">
            <Element Name="Cleared" Column="0" />
            <Element Name="Monitor SMC" Column="0" />
            <Element Name="Pushback" Column="0" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="1" />
            <Element Name="Runway" Column="1" />
            <Element Name="Circuit" Column="2" />
            <Element Name="Departed" Column="2" />
            <Element Name="Arrivals" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="ADC">
            <Element Name="Holding Point" Column="0" />
            <Element Name="Runway" Column="1" />
            <Element Name="Circuit" Column="1" />
            <Element Name="Departed" Column="2" />
            <Element Name="Arrivals" Column="2" />
        </LayoutDefinition>
        <!-- METRO D -->
        <LayoutDefinition Name="All" Type="D">
            <Element Name="Preactive" Column="0" />
            <Element Name="Taxi" Column="0" />
            <Element Name="Holding Point" Column="1" />
            <Element Name="Runway" Column="1" />
            <Element Name="Circuit" Column="2" />
            <Element Name="Arrivals" Column="2" />
        </LayoutDefinition>
        <!-- PROC -->
        <LayoutDefinition Name="All" Type="Proc">
            <Element Name="Preactive" Column="0" />
            <Element Name="Cleared" Column="0" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="1" />
            <Element Name="Runway" Column="1" />
            <Element Name="Active" Column="2" />
            <Element Name="Pending Arrivals" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="SMC" Type="Proc">
            <Element Name="Preactive" Column="0" />
            <Element Name="Cleared" Column="0" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="2" />
            <Element Name="Runway" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="ADC" Type="Proc">
            <Element Name="Holding Point" Column="0" />
            <Element Name="Runway" Column="0" />
            <Element Name="Active" Column="1" />
            <Element Name="Pending Arrivals" Column="2" />
        </LayoutDefinition>
        <!-- Proc Push -->
        <LayoutDefinition Name="All" Type="ProcPush">
            <Element Name="Preactive" Column="0" />
            <Element Name="Cleared" Column="0" />
            <Element Name="Pushback" Column="0" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="1" />
            <Element Name="Runway" Column="1" />
            <Element Name="Active" Column="2" />
            <Element Name="Pending Arrivals" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="SMC" Type="ProcPush">
            <Element Name="Preactive" Column="0" />
            <Element Name="Cleared" Column="0" />
            <Element Name="Pushback" Column="1" />
            <Element Name="Taxi" Column="1" />
            <Element Name="Holding Point" Column="2" />
            <Element Name="Runway" Column="2" />
        </LayoutDefinition>
        <LayoutDefinition Name="ADC" Type="ProcPush">
            <Element Name="Holding Point" Column="0" />
            <Element Name="Runway" Column="0" />
            <Element Name="Active" Column="1" />
            <Element Name="Pending Arrivals" Column="2" />
        </LayoutDefinition>

    </LayoutDefinitions>


